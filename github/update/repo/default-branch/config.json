{
  "dockerImageBuilder": "ritclizup/rit-python3-builder",
  "inputs": [
    {
      "name": "rit_git_token",
      "type": "CREDENTIAL_GITHUB_TOKEN"
    },
    {
      "label": "Repository owner:",
      "name": "rit_repo_owner",
      "tutorial": "Ex: octocat",
      "type": "text",
      "required": true,
      "cache": {
        "active": true,
        "qty": 5,
        "newLabel": "Type new value:"
      }
    },
    {
      "label": "Repository name:",
      "name": "rit_git_repo",
      "tutorial": "Ex: ritchie-formulas",
      "type": "text",
      "required": true,
      "cache": {
        "active": true,
        "qty": 5,
        "newLabel": "Type new value:"
      }
    },
    {
      "label": "Branch to set as default:",
      "name": "rit_repo_branch",
      "tutorial": "The branch must already exist (Ex: dev, qa, test)",
      "type": "text",
      "required": true,
      "cache": {
        "active": true,
        "qty": 5,
        "newLabel": "Type new value:"
      }
    }
  ]
}
