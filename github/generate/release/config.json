{
  "dockerImageBuilder": "ritclizup/rit-python3-builder",
  "inputs": [
    {
      "name": "rit_github_token",
      "type": "CREDENTIAL_GITHUB_TOKEN"
    },
    {
      "label": "Repository owner:",
      "name": "rit_repo_owner",
      "tutorial": "Ex: octocat",
      "type": "text",
      "required": true,
      "cache": {
        "active": true,
        "qty": 5,
        "newLabel": "Type new value:"
      }
    },
    {
      "label": "Repository name:",
      "name": "rit_github_repo",
      "tutorial": "Ex: ritchie-formulas",
      "type": "text",
      "required": true,
      "cache": {
        "active": true,
        "qty": 5,
        "newLabel": "Type new value:"
      }
    },
    {
      "label": "Branch:",
      "name": "rit_repo_branch",
      "tutorial": "Ex: master, main, qa",
      "type": "text",
      "required": true,
      "cache": {
        "active": true,
        "qty": 5,
        "newLabel": "Type new value:"
      }
    },
    {
      "label": "Version Tag:",
      "name": "rit_repo_version",
      "tutorial": "Ex: 2.1.0",
      "type": "text",
      "required": true
    }
    ,
    {
      "label": "Description:",
      "name": "rit_release_description",
      "tutorial": "Ex: First release",
      "type": "text",
      "default": "Release generated automatically using Ritchie CLI"
    }
  ]
}
